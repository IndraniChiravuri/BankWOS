@model  IEnumerable<BusinessLayer.Manager>

@{
    ViewBag.Title = "Index";
}
<div class="jumbotron bg-light text-dark">
    
    <h3>Managers List</h3>
    <label id="message"></label>
    <div class="table-responsive">
        <div>

            <table border="1" style="font-family:Arial" class="table table-hover table-striped">
                <tr>
                    <th>Manager Id</th>
                    <th>Manager Name</th>
                    <th>Branch Id</th>
                </tr>

                @foreach (BusinessLayer.Manager manager in @Model)
                {
                    <tr>

                        <td>
                            @manager.managerId
                        </td>
                        <td>
                            @manager.managerName
                        </td>
                        <td>
                            @manager.branchId
                        </td>
                    </tr>
                }

            </table>
            <br />

        </div>
    </div>
</div>